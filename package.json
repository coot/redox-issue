{
  "name": "redox-issue",
  "version": "1.0.0",
  "main": "\"\"",
  "repository": "https://github.com/spicydonuts/redox-issue",
  "author": "Michael Trotter",
  "license": "MIT",
  "private": true,
  "scripts": {
    "postinstall": "bower install",
    "build": "pulp build",
    "build-watch": "pulp -w build",
    "prestart": "mkdir -p output/Main && touch output/Main/index.js",
    "start": "run-p start-rollup start-dev-server",
    "start-purs": "pulp -w build",
    "start-rollup": "rollup -cw",
    "start-dev-server": "./bin/dev-server",
    "test": "pulp test",
    "prebundle": "rm -rf output dce-output dist/bundle.js",
    "bundle": "run-s build-purs build-zephyr build-rollup",
    "bundle-purs": "./bin/purs compile --dump-corefn \"./bower_components/purescript-*/src/**/*.purs\" \"./src/**/*.purs\"",
    "bundle-zephyr": "./bin/zephyr -f Main.main",
    "bundle-rollup": "NODE_ENV=production rollup -c"
  },
  "devDependencies": {
    "bower": "^1.8.2",
    "browser-sync": "^2.18.13",
    "express": "^4.15.4",
    "http-proxy-middleware": "^0.17.4",
    "http2": "^3.3.7",
    "pulp": "^12.0.1",
    "purescript": "^0.11.6",
    "purescript-psa": "^0.5.1",
    "rollup": "^0.50.0",
    "rollup-plugin-commonjs": "^8.2.1",
    "rollup-plugin-gzip": "^1.3.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^2.0.1",
    "send": "^0.15.6",
    "webpack": "^3.6.0",
    "yarn-run-all": "^3.1.1"
  },
  "dependencies": {}
}
